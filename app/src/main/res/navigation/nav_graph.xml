<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/default_fragment">

    <fragment
        android:id="@+id/default_fragment"
        android:name="com.combyne.android.tvshowmanager.splash.controllers.SplashFragment"
        android:label="fragment_default"
        tools:layout="@layout/fragment_splash" />

    <dialog
        android:id="@+id/add_movies_bottom_dialog_fragment"
        android:name="com.combyne.android.tvshowmanager.addmovie.controllers.AddMovieBottomSheetDialog">
        <argument android:name="arg" android:defaultValue="@null" />
    </dialog>
    <fragment
        android:id="@+id/movies_fragment"
        android:name="com.combyne.android.tvshowmanager.movies.controller.MoviesFragment"
        android:label="AllShowsFragment" />
</navigation>